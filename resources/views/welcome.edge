<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <title>Hello Adonis</title>
  <link rel="stylesheet" href="/css/style.css">
</head>
<body>
  <header>
    <div class="logo">
      freshGear
    </div>
    <div class="menu">
        <a href="#">About</a>
        <a href="#">New Arrival</a>
        <a href="#">Categories</a>
        <a href="#" class="cart-link">Cart<div>4</div></a>
    </div>
  </header>
  <section class="content-area home-page">
      <div class="top-section">
        <div class="product">
            <div class="circle">
                <img src="img/products/adidas-yeezy-boost-white.png" alt="">
              </div>
        </div>
        <div class="titles">
            <h3>yeezy</h3>
            <h1>Boost 350</h1>
          </div>
      </div>
    </section>

    <section id="cart-popup" class="active">
      <div>
          <div class="title">My Cart</div>
      </div>
      <div class="cart-items">
        <div class="item">
          <img src="img/products/adidas-yeezy-boost-white.png" alt="">
          <div class="delete-btn">
            <div class="circle">X</div>
          </div>
        </div>
      </div>
      <div class="product-total">
          <div class="title">Total</div>
          <div class="number">x4</div>
      </div>
      <div class="price-total">
        <div class="title">Total</div>
        <div class="number">$ 3,000</div>
      </div>
      <div class="checkout">Checkout</div>
    </section>
  
  <script src="/js/dist/vendors~FirstComp.js"></script>
  <script src="/js/dist/FirstComp.js"></script>
</body>
</html>
